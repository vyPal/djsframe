{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["src", "src/commands", "src/providers/sqlite.js", "README.md"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/docdash",
    "destination": "docs/"
  }
}